Public Class Student

    Public m_age As Integer
    Public m_avg_grade As Integer
    Public m_number_of_exams As Integer
    Public m_name As String
    Public m_ID As UInteger

    Public Sub New(age As Integer, avg_grade As Integer, n_exams As Integer)
        m_age = age
        m_avg_grade = avg_grade
        m_number_of_exams = n_exams
    End Sub

End Class

